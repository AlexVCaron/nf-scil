process {
    withName: "REGISTRATION_ANTS" {
        ext.quick = true
        ext.threads = 1
        ext.random_seed = 44
        publishDir = { "${params.outdir}/${task.process.tokenize(':')[-1].tokenize('_')[0].toLowerCase()}" }
    }
    withName: "BUNDLE_RECOGNIZE" {
        publishDir = { "${params.outdir}/${task.process.tokenize(':')[-1].tokenize('_')[0].toLowerCase()}" }
        ext.minimal_vote_ratio = 0.6
        ext.rbx_processes = 4
        ext.seed = 1234
        ext.outlier_alpha = 0.4
    }
}